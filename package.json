        "typescript": "5.6.3",
        "unocss": "0.65.1",
        "unplugin-auto-import": "0.18.6",
        "unplugin-svg-component": "0.12.1",
        "unplugin-vue-components": "0.27.5",
        "vite": "6.0.3",
        "vite-svg-loader": "5.1.0",
        "vitest": "2.1.8",
        "vue-tsc": "2.1.10"
    },
    "lint-staged": {
        "*": "eslint --fix"
    }
