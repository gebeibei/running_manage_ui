{
    "name": "running_manage_ui",
    "type": "module",
    "version": "5.0.0-beta.0",
    "description": "A crafted admin template, built with Vue3, Vite, TypeScript, Element Plus, and more",
    "author": "gebb <362194581@qq.com> (https://github.com/gebeibei)",
    "repository": "https://github.com/gebeibei/running_manage_ui",
    "scripts": {
        "dev": "vite",
        "build:staging": "vue-tsc && vite build --mode staging",
        "build": "vue-tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint . --fix",
        "prepare": "husky",
        "test": "vitest",
        "data:clean": "rm -f run_page/data.db {GPX,TCX,FIT}_OUT/* activities/* src/data/activities.json",
        "data:analysis": "python3 run_page/gen_svg.py --from-db --type github --output src/common/assets/icons/data-statis/github.svg"
    },
    "dependencies": {
        "@element-plus/icons-vue": "2.3.1",
        "@mapbox/mapbox-gl-language": "1.0.1",
        "@mapbox/polyline": "1.2.1",
        "@surbowl/world-geo-json-zh": "2.1.4",
        "axios": "1.7.9",
        "dayjs": "1.11.13",
        "element-plus": "2.9.0",
        "gcoord": "1.0.6",
        "geojson": "0.5.0",
        "js-cookie": "3.0.5",
        "lodash-es": "4.17.21",
        "mapbox-gl": "3.9.1",
        "mitt": "3.0.1",
        "normalize.css": "8.0.1",
        "nprogress": "0.2.0",
        "path-browserify": "1.0.1",
        "path-to-regexp": "8.2.0",
        "pinia": "2.3.0",
        "screenfull": "6.0.2",
        "viewport-mercator-project": "7.0.4",
        "vue": "3.5.13",
        "vue-router": "4.5.0",
        "vxe-table": "4.6.25"
    },
    "devDependencies": {
        "@antfu/eslint-config": "3.11.2",
        "@types/geojson": "7946.0.15",
        "@types/js-cookie": "3.0.6",
        "@types/lodash-es": "4.17.12",
        "@types/mapbox__polyline": "1.0.5",
        "@types/node": "22.10.1",
        "@types/nprogress": "0.2.3",
        "@types/path-browserify": "1.0.3",
        "@unocss/preset-rem-to-px": "0.65.2",
        "@vitejs/plugin-vue": "5.2.1",
        "@vitejs/plugin-vue-jsx": "4.1.1",
        "@vue/test-utils": "2.4.6",
        "eslint": "9.16.0",
        "eslint-plugin-format": "0.1.3",
        "happy-dom": "15.11.7",
        "husky": "9.1.7",
        "lint-staged": "15.2.10",
